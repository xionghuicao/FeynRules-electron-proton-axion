(* ************************** *)
(* ***** Parameters    ***** *)
(* ************************** *)

M$Parameters = {

    ee == {
        ParameterType    -> External,
        Value            -> Sqrt[4.0*Pi*1.0/137.0],
        InteractionOrder -> {qed, 1}, 
        TeX              -> "ee photon electron electron",
        Description      -> "Electric coupling constant"
    },

    f1 == {
        ParameterType    -> External,
        Value            -> 1,
        InteractionOrder -> {qed,0}, 
        TeX              -> "f1 photon proton proton",
        Description      -> "f1 coupling constant"
    },

    f2 == {
        ParameterType    -> External,
        Value            -> 1.793,
        InteractionOrder -> {qed, 0}, 
        TeX              -> "f2 photon proton proton",
        Description      -> "f2 coupling constant"
    },

    gaxpp == {
        ParameterType    -> External,
        Value            -> 0.215,
        InteractionOrder -> {order1, 1}, 
        TeX              -> "g axion proton proton",
        Description      -> "axpp coupling constant"
    },

    gaxaa == {
        ParameterType    -> External,
        Value            -> 0.005588,
        InteractionOrder -> {order1, 1}, 
        TeX              -> "g axion photon photon",
        Description      -> "axaa coupling constant"
    },

    grhopp1 == {
        ParameterType    -> External,
        Value            -> 3.25,
        InteractionOrder -> {order1, 1}, 
        TeX              -> "g axion proton proton 1",
        Description      -> "rhopp1 coupling constant"
    },

    grhopp2 == {
        ParameterType    -> External,
        Value            -> -10.6,
        InteractionOrder -> {order1, 1}, 
        TeX              -> "g rho proton proton 2",
