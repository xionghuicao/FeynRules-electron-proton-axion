    }, 


    grhoaxa == {
        ParameterType    -> External,
        Value            -> -0.098,
        InteractionOrder -> {order1, 1}, 
        TeX              -> "g rho axion photon",
        Description      -> "rho ax a coupling constant"
    },

  gomegapp == {
        ParameterType    -> External,
        Value            -> 11.7,
        InteractionOrder -> {order2, 1}, 
        TeX              -> "g omega proton proton",
        Description      -> "omega p p coupling constant"
    },

    gomegaaxa == {
        ParameterType    -> External,
