(* ************************** *)
(* ***** Lagrangian    ***** *)
(* ************************** *)

FA[mu_,nu_] := del[A[mu], nu] - del[A[nu], mu];

Frho[mu_,nu_] := del[rho[mu], nu] - del[rho[nu], mu];

Fomega[mu_,nu_] := del[omega[mu], nu] - del[omega[nu], mu];


sigma[mu_,nu_]:= 1/2*I*(Ga[mu].Ga[nu] - Ga[nu].Ga[mu]);


Leea = ee* ebar.Ga[mu].e*A[mu];

Lppa1= -f1*ee* pbar.Ga[mu].p*A[mu];

Lppa2 = f2*ee*1/(2mp)* pbar.sigma[mu,nu].p *del[A[mu],nu];

Lppax = gaxpp* pbar.Ga[mu].Ga[5].p*del[ax,mu];

Laxaa = gaxaa* Eps[mu, nu, lambda, kappa] FA[mu,nu] *FA[lambda,kappa] *ax;

Lpprho1 = -grhopp1 * pbar.Ga[mu].p *rho[mu];

Lpprho2 = grhopp2/2 * pbar.sigma[mu, nu].p *del[rho[mu],nu];

Lrhoaxa = 1/4* ee* grhoaxa * Eps[mu, nu, lambda, kappa]*Frho[mu,nu]*FA[lambda,kappa] *ax;

Lppomega = -gomegapp* pbar. Ga[mu].p*omega[mu];

Lomegaaxa = 1/4* ee*gomegaaxa * Eps[mu, nu, lambda, kappa]*Fomega[mu,nu]*FA[lambda,kappa] *ax;


Lag = Leea + Lppa1 + Lppa2 + Lppax + Laxaa + Lpprho1 + Lpprho2 + Lrhoaxa + Lppomega + Lomegaaxa; 



M$Lagrangians = {Lag};

(* End of model *)
